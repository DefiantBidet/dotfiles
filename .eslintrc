{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true,
  },
  "env": {
    "node": true,
    "es6": true
  },
  "plugins": [
      "eslint-plugin-react"
  ],
  "rules": {
    "block-scoped-var": 2,
    "camelcase": 0,
    "comma-dangle": [ 1, "always-multiline" ],
    "comma-style": [ 2 ],
    "comma-spacing": 0,
    "curly": [ 2, "all" ],
    "eol-last": [1],
    "eqeqeq": [ 2, "allow-null" ],
    "global-strict": [ 0, "never" ],
    "guard-for-in": 0,
    "key-spacing": 0,
    "new-cap": 1,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-cond-assign": [ 2, "except-parens" ],
    "no-empty": 2,
    "no-extend-native": 2,
    "no-extra-parens": 0,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-multi-str": 2,
    "no-multi-spaces": 0,
    "no-new": 2,
    "no-proto": 2,
    "no-redeclare": 1,
    "no-sequences": 2,
    "no-shadow": 0,
    "no-undef": 1,
    "no-underscore-dangle": 0,
    "no-unused-vars": 1,
    "no-use-before-define": [ 2 , "nofunc" ],
    "no-with": 2,
    "no-mixed-requires": 0,
    "quotes": [ 1, "single" ],
    "react/jsx-uses-vars": 1,
    "semi": [2, "always"],
    "valid-typeof": 2,
    "wrap-iife": [ 2, "inside" ]
  }
}
